{
  "name": "yamnick",
  "version": "0.0.1",
  "description": "Diagram collaboration app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/detoix/yamnick.git"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test-silent": "jest --verbose --silent"
  },
  "author": "detoix",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/mocks.js"
    }
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.13.5",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "konva": "^7.2.2",
    "nact": "^7.3.4",
    "nact-persistence-postgres": "^7.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-konva": "^16.13.0-6",
    "react-router-dom": "^5.2.0",
    "short-uuid": "^4.1.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "socketio-jwt": "^4.6.2",
    "use-image": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
